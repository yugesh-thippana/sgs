{
  "manifest_version": 2,
  "name": "sgs",
  "description": "sgs",
  "version": "1.1.1",
  "icons": {
    "128":"icon.png"
    },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "sgs"
  },
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js", "worker.js"],
    "persistent": true
  },
  "permissions": [
    "tabCapture",
    "downloads",
    "storage"
  ]
}
